{
  "name": "youtube-ar",
  "scripts": {
    "postdeploy": "psql youtube-ar < schema.sql"
  },
  "env": {
    "AWS_ACCESS_KEY_ID": {
      "required": true
    },
    "AWS_REGION": {
      "required": true
    },
    "AWS_SECRET_ACCESS_KEY": {
      "required": true
    },
    "PAPERTRAIL_API_TOKEN": {
      "required": true
    },
    "S3_BUCKET": {
      "required": true
    }
  },
  "formation": {
  },
  "addons": [
    "papertrail",
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/go"
    },
    {
      "url": "https://github.com/heroku/heroku-buildpack-apt"
    },
    {
      "url": "https://github.com/yansal/heroku-buildpack-youtube-dl"
    }
  ]
}
